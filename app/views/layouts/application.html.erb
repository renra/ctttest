<!DOCTYPE html>
<html>
<head>
  <title>Ctttest</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<% if RAILS_ENV=='development' %>
  <div>
    <%= session.inspect %>
  </div>
<% end %>


<% if flash[:notice] %>
  <div>
    <p id="notice"><%= notice %></p>
  </div>
<% end %>

<% if flash[:error] %>
  <div>
    <p id="error"><%= flash[:error] %></p>
  </div>
<% end %>

<div>
  <% if current_user %>
    <%= link_to 'Logout', logout_path %>
  <% end %>
</div>

<%= yield %>

<% if current_user %>
  <div class="bottom_links">
    <%= link_to 'Projects', projects_path %> <br />
    <% if can? :create, User %>
     <%= link_to 'Team Members', users_path %><br />
    <% end %>
  </div>
<% end %>

</body>
</html>
